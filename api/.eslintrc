{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "env": {
    "browser": false,
    "node": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "function-paren-newline": ["error", "consistent"],
    "no-underscore-dangle": ["error", { "allowAfterThis": true, "allow": ["_id"] }]
  }
}
